/**
 * This script changes all requests' User-Agent to iOS Safari
 * Change myUserAgent below to apply another UA
 */
{
  "action": {
    "type": "modifyHeaders",
    "responseHeaders": [
      {
        "header": "User-Agent",
        "operation": "set",
        "value": "Mozilla/5.0 (iPhone; CPU iPhone OS 11_0 like Mac OS X) AppleWebKit/604.1.38 (KHTML, like Gecko) Version/11.0 Mobile/15A372 Safari/604.1"
      }
    ]
  }
}
